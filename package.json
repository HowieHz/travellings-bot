{
	"scripts": {
		"lint": "tsc --noEmit && eslint ./src",
		"fix": "eslint src --fix",
		"dev": "unbuild --stub && node dist/app.mjs",
		"dev-public": "unbuild --stub && node dist/app.mjs -public-mode",
		"dev-cli": "unbuild --stub && node dist/app.mjs -cli",
		"build": "unbuild",
		"cli": "unbuild && node dist/app.mjs -cli",
		"prebuild": "eslint src",
		"start": "node dist/app.mjs",
		"format": "pnpm run \"/^format:.*/\"",
		"format:prettier": "prettier --write \"*.{js,ts,json,yaml,md,cjs,mjs}\" \"./src/**/*.{js,ts,css,json,ml,yaml,html}\"",
		"prepare": "husky"
	},
	"dependencies": {
		"@larksuiteoapi/node-sdk": "^1.38.0",
		"axios": "^1.7.4",
		"chalk": "^4.1.0",
		"dotenv": "^16.3.1",
		"moment-timezone": "^0.5.45",
		"mysql2": "^3.11.4",
		"node-cron": "^3.0.3",
		"puppeteer": "^22.15.0",
		"redis": "^4.6.13",
		"sequelize": "^6.35.2",
		"telegraf": "^4.15.3"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.6",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^22.7.5",
		"@types/node-cron": "^3.0.11",
		"concurrently": "^9.0.1",
		"eslint": "^9.20.0",
		"eslint-import-resolver-typescript": "^3.7.0",
		"eslint-plugin-import-x": "^4.6.1",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^15.12.0",
		"husky": "^9.1.7",
		"prettier": "^3.4.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.23.0",
		"unbuild": "^3.3.1"
	},
	"packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
